{
  "name": "playwright",
  "version": "0.0.1",
  "description": "PLaywright test for Vercel examples",
  "license": "MIT",
  "scripts": {
    "build:generate-tests": "swc scripts -d dist/scripts -s",
    "generate-tests": "pnpm build:generate-tests && NODE_OPTIONS=--enable-source-maps node dist/scripts/generate-tests.js",
    "clean-tests": "rm -rf src/e2e/tests/generated",
    "test": "TEST_TYPE=e2e playwright test --config src/playwright.config.ts"
  },
  "devDependencies": {
    "@playwright/test": "^1.25.2",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.2",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "front-matter": "^4.0.2",
    "sluga": "^0.2.1"
  }
}
